{
  "name": "webpack-boilerplate-2018",
  "sideEffects": [
    "*.scss"
  ],
  "version": "1.0.0",
  "description": "Webpack 4 JS/SCSS Boilerplate",
  "private": true,
  "scripts": {
    "test": "jest",
    "debug": "NODE_ENV=debug npm test",
    "develop": "NODE_ENV=develop",
    "start": "webpack-dev-server --config webpack.dev.js --open-page index.html",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "yarn eslint .",
    "lint:fix": "yarn lint --fix",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "fix-styles": "prettier-stylelint --write 'src/**/*.{css,scss}' "
  },
  "author": "czechue",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "deepmerge": "^2.2.1",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "external-svg-sprite-loader": "^3.4.1",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.4.0",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "jest-puppeteer": "^3.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "pngjs": "^3.3.3",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "pug-html-loader": "^1.1.5",
    "puppeteer": "^1.5.0",
    "raw-loader": "^0.5.1",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint-scss": "^3.3.1",
    "svg-sprite-loader": "^4.1.1",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "modularscale-sass": "^3.0.8",
    "normalize-scss": "^7.0.1",
    "pixelmatch": "^4.0.2",
    "regenerator-runtime": "^0.11.1",
    "uikit": "^3.0.0-rc.17",
    "zepto": "^1.2.0",
    "zepto-webpack": "^1.2.1"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  }
}
