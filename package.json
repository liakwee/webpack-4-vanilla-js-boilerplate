{
  "name": "webpack-boilerplate-2018",
  "sideEffects": [
    "*.scss"
  ],
  "version": "1.0.0",
  "description": "Webpack 4 JS/SCSS Boilerplate",
  "private": true,
  "scripts": {
    "test": "jest",
    "debug": "NODE_ENV=debug npm test",
    "develop": "NODE_ENV=develop",
    "start": "cross-env NODE_ENV=development  webpack-dev-server --config ./config/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "lint": "yarn eslint .",
    "lint:fix": "yarn lint --fix",
    "fix-code": "prettier-eslint --write 'src/scripts/**/*.{js,jsx}'",
    "fix-styles": "prettier-stylelint --write 'src/styles/**/*.{css,scss}'",
    "postinstall": "node ./config/site.setup.js"
  },
  "author": "czechue",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^1.0.1",
    "clear": "^0.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "deepmerge": "^3.1.0",
    "enquirer": "^2.3.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "faker": "^4.1.0",
    "figlet": "^1.2.1",
    "file-loader": "^3.0.1",
    "globby": "^9.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pngjs": "^3.3.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "pug-html-loader": "^1.1.5",
    "puppeteer": "^1.5.0",
    "robotstxt-webpack-plugin": "^5.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.23.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^3.3.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.1.1",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webapp-webpack-plugin": "^2.6.0",
    "webpack": "4.28.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "webpackbar": "^3.1.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "jquery": "^3.3.1",
